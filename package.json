{
  "name": "@moccona/openapi",
  "version": "0.0.0",
  "description": "Code gen for OpenAPI/Swagger",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsup --watch",
    "build": "cross-env NODE_ENV=production tsup",
    "test": "cross-env NODE_ENV=test vitest --watch false",
    "test:codegen": "cross-env NODE_ENV=test vitest --watch false -t codegen"
  },
  "keywords": [
    "utils",
    "code-gen",
    "client APIs"
  ],
  "author": "freemode",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./npm/*.d.ts",
      "import": "./npm/*.js",
      "require": "./npm/*.cjs"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "dependencies": {
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "undici": "^7.2.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@moccona/eslint-config": "^0.3.12",
    "@moccona/logger": "^0.0.1",
    "@moccona/prettierrc": "^0.0.2",
    "@types/node": "^20.17.11",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "fast-deep-equal": "^3.1.3",
    "openapi-types": "^12.1.3",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "prettier": "@moccona/prettierrc",
  "packageManager": "pnpm@9.7.1"
}