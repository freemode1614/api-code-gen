{
  "name": "@moccona/api-codegen-vite-plugin",
  "version": "0.0.1",
  "description": "Code generator base on typescript AST tools",
  "type": "module",
  "author": "freemode",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./npm/*.d.ts",
      "import": "./npm/*.js",
      "require": "./npm/*.cjs"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@moccona/api-codegen-openapi": "workspace:*",
    "@moccona/logger": "^0.0.2"
  },
  "devDependencies": {
    "@moccona/eslint-config": "^0.3.12",
    "@moccona/preset-config": "^0.0.2",
    "@types/node": "^22.13.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
