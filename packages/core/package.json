{
  "name": "@moccona/gen-core",
  "version": "0.0.0",
  "description": "Code generator base on typescript AST tools",
  "type": "module",
  "author": "freemode",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./npm/*.d.ts",
      "import": "./npm/*.js",
      "require": "./npm/*.cjs"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsup --watch",
    "build": "cross-env NODE_ENV=production tsup"
  },
  "dependencies": {
    "@moccona/logger": "^0.0.2",
    "openapi-types": "^12.1.3"
  },
  "devDependencies": {
    "@moccona/eslint-config": "^0.3.12",
    "@moccona/preset-config": "^0.0.2",
    "@types/node": "^22.10.7",
    "cross-env": "^7.0.3",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.2"
  },
  "prettier": "@moccona/preset-config/prettierrc",
  "packageManager": "pnpm@9.7.1"
}
