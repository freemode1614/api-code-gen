{
  "name": "@moccona/api-codegen",
  "version": "0.0.0",
  "description": "Code generator base on typescript AST tools",
  "type": "module",
  "license": "MIT",
  "author": "freemode",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./npm/*.d.ts",
      "import": "./npm/*.js",
      "require": "./npm/*.cjs"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsup --watch",
    "build": "cross-env NODE_ENV=production tsup"
  },
  "prettier": "@moccona/preset-config/prettierrc",
  "packageManager": "pnpm@9.7.1",
  "browserlist": [
    "not op_mini"
  ],
  "dependencies": {
    "@moccona/logger": "^0.0.2",
    "openapi-types": "^12.1.3",
    "typescript": "^5.7.3",
    "undici": "^7.3.0"
  },
  "devDependencies": {
    "@moccona/eslint-config": "^0.3.12",
    "@moccona/preset-config": "^0.0.3",
    "@types/node": "^22.13.1",
    "cross-env": "^7.0.3",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6"
  }
}

